// for else
- var users = [];

ul
    for user in users
        li= user.name
    else
        li no users!


// if + for else
- var users = [{ name: 'tobi', friends: ['loki'] }, { name: 'loki' }]

if users
    ul
        for user in users
            li= user.name
        else
            li no users!


// each + else
- var user = { name: 'tobi', age: 10 }

ul
    each val, key in user
        li #{key}: #{val}
    else
        li user has no details!


// each + else
- var user = {}

ul
    each prop, key in user
        li #{key}: #{val}
    else
        li user has no details!


// each + else
- var user = Object.create(null)
- user.name = 'tobi'

ul
    each val, key in user
        li #{key}: #{val}
    else
        li user has no details!


// each + else
- var numbers = [2, 4, 6, 8, 10];

ul
    each val, key in numbers
        li #{val}: #{key}
